{
  "name": "code-completion",
  "displayName": "code-completion",
  "description": "An extension that provides code completion through a LLM as well as through chats",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codeCompletions",
          "title": "Code Completions",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "codeCompletions": [
        {
          "type": "webview",
          "id": "code-completion.webview",
          "name": "Code Completion",
          "icon": "media/icon.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/assert": "^1.5.10",
    "@types/mocha": "^10.0.7",
    "@types/vscode": "^1.93.0",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-web": "^0.0.58",
    "assert": "^2.1.0",
    "eslint": "^9.9.1",
    "mocha": "^10.7.3",
    "process": "^0.11.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
